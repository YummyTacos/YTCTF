{
    "name": "ytctf-platform-frontend",
    "version": "1.0.0",
    "private": true,
    "scripts": {
        "serve": "vue-cli-service serve",
        "prebuild": "npm run check-updates-favicon",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint",
        "generate-favicon": "real-favicon generate favicon.json favicon-data.json public",
        "inject-favicon": "real-favicon inject favicon-data.json public public/index.html",
        "check-updates-favicon": "real-favicon check-for-update --fail-on-update favicon-data.json",
        "favicon": "npm run generate-favicon && npm run inject-favicon"
    },
    "dependencies": {
        "acorn": "^6.2.0",
        "bootstrap": "^4.3.1",
        "bs-custom-file-input": "^1.3.2",
        "core-js": "^2.6.5",
        "jquery": "^3.4.1",
        "popper.js": "^1.15.0",
        "showdown": "^1.9.0",
        "simplemde": "^1.11.2",
        "vue": "^2.6.10",
        "vue-router": "^3.0.6",
        "vue-showdown": "^2.4.1",
        "vuex": "^3.1.1"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "babel-eslint": "^10.0.1",
        "cli-real-favicon": "0.0.8",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "node-sass": "^7.0.0",
        "sass-loader": "^7.1.0",
        "vue-template-compiler": "^2.6.10",
        "webpack": "^4.35.2"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/essential",
            "eslint:recommended"
        ],
        "rules": {},
        "parserOptions": {
            "parser": "babel-eslint"
        }
    },
    "postcss": {
        "plugins": {
            "autoprefixer": {}
        }
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead"
    ]
}
